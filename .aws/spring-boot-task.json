{
  "containerDefinitions": [
    {
      "name": "springboot",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "family": "Test",
  "executionRoleArn": "arn:aws:iam::444971044603:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "2048",
  "memory": "950",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
